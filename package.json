{
  "name": "dffoo_gateway",
  "version": "1.0.6",
  "description": "DFFOO Clients & Gateway Proxy",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "build": "node build.win.js",
    "build:mac:x64": "node build.mac.x64.js && codesign --sign -o dffoo_ios_signer-macos-x64",
    "build:mac:arm": "node build.mac.arm.js && codesign --sign -o dffoo_ios_signer-macos-arm64",
    "build:linux": "node build.linux.js",
    "build:win": "node build.win.js",
    "note": "echo \"Only build for the machine type you are running. Mac needs to be signed on Mac in order to run on Mac.\""
  },
  "bin": {
    "app": "app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hearhellacopters/dffoo_gateway.git"
  },
  "author": "HearHellacopters",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hearhellacopters/dffoo_gateway/issues"
  },
  "homepage": "https://github.com/hearhellacopters/dffoo_gateway#readme",
  "dependencies": {
    "@hearhellacopters/exe": "^2.1.3",
    "@types/node": "^20.14.11",
    "@yao-pkg/pkg": "^5.15.0",
    "archiver": "^6.0.1",
    "bireader": "^1.0.56",
    "hash-maker": "^1.0.12",
    "keypress": "^0.2.1",
    "progress": "^2.0.3",
    "readline": "^1.3.0",
    "yauzl": "^2.10.0",
    "eslint": "^9.4.0",
    "eslint-plugin-jsdoc": "^48.2.7"
  }
}