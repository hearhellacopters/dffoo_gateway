{
  "name": "dffoo_clients",
  "version": "1.0.4",
  "description": "DFFOO Clients & Gateway Proxy",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "build": "node build:win.js",
    "build:win": "node src/build.win.js",
    "build:linux": "node src/build.linux.js",
    "build:macarm": "node src/build.mac.arm.js",
    "build:macx64": "node src/build.mac.64.js",
    "note": "echo \"Only build for the machine type you are running. Mac needs to be signed on Mac in order to run on Mac.\""
  },
  "pkg": {
    "scripts": "config/**/*",
    "targets": [
      "node16-win-x64",
      "node16-linux-x64",
      "node18-macos-x64",
      "node18-macos-arm64"
    ],
    "outputPath": "./"
  },
  "bin": {
    "app": "src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hearhellacopters/dffoo_gateway.git"
  },
  "author": "HearHellacopters",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hearhellacopters/dffoo_gateway/issues"
  },
  "homepage": "https://github.com/hearhellacopters/dffoo_gateway#readme",
  "dependencies": {
    "@angablue/exe": "^2.1.3",
    "archiver": "^6.0.1",
    "bireader": "^1.0.56",
    "hash-maker": "^1.0.12",
    "keypress": "^0.2.1",
    "pkg": "^5.8.1",
    "progress": "^2.0.3",
    "readline": "^1.3.0",
    "yauzl": "^2.10.0"
  }
}
